@model CakeShop.Models.ViewModel.PieListViewModel

<!DOCTYPE html>
<html>
<head>
    <title>Grapes Cake Shop</title>
</head>
<body>
    <h3>@Model.CurrentCategory</h3>
    @foreach (var pie in Model.Pies)
    {
    <div>
        <h2>@pie.Name</h2>
        <h3>@pie.Price.ToString("c")</h3>
        <h3>@pie.Category.CategoryName</h3>
    </div>
    }
</body>
</html>